        $this->houseMaterial = $houseMaterial;

        return $this;
    }

    /**
     * Get houseMaterial
     *
     * @return integer 
     */
    public function getHouseMaterial()
    {
        return $this->houseMaterial;
    }

    /**
     * Set videoId
     *
     * @param integer $videoId
     * @return ChHouse
     */
    public function setVideoId($videoId)
    {
        $this->videoId = $videoId;

        return $this;
    }

    /**
     * Get videoId
     *
     * @return integer 
     */
    public function getVideoId()
