<?php

namespace Torgovorot\TorgBundle\Entity;

use Doctrine\ORM\Mapping as ORM;

/**
 * ChHouse
 */
class ChHouse
{
    /**
     * @var integer
     */
    private $houseType;

    /**
     * @var integer
     */
    private $planing;

    /**
     * @var float
     */
    private $squareHouse;

    /**
     * @var float
     */
    private $squareEarth;

    /**
     * @var float
     */
    private $distanceToCity;

    /**
     * @var integer
     */
    private $convinient;

    /**
     * @var integer
     */
    private $canalization;

    /**
     * @var integer
     */
    private $water;

    /**
     * @var integer
     */
    private $electricity;

    /**
     * @var integer
     */
    private $gas;

    /**
     * @var integer
     */
    private $houseMaterial;

    /**
     * @var integer
     */
    private $videoId;

    /**
     * @var integer
     */
    private $photoId;

    /**
     * @var integer
     */
    private $id;


    /**
     * Set houseType
     *
     * @param integer $houseType
     * @return ChHouse
     */
    public function setHouseType($houseType)
    {
        $this->houseType = $houseType;

        return $this;
    }

    /**
     * Get houseType
     *
     * @return integer 
     */
    public function getHouseType()
    {
        return $this->houseType;
    }

    /**
     * Set planing
     *
     * @param integer $planing
     * @return ChHouse
     */
    public function setPlaning($planing)
    {
        $this->planing = $planing;

        return $this;
    }

    /**
     * Get planing
     *
     * @return integer 
     */
    public function getPlaning()
    {
        return $this->planing;
    }

    /**
     * Set squareHouse
     *
     * @param float $squareHouse
     * @return ChHouse
     */
    public function setSquareHouse($squareHouse)
    {
        $this->squareHouse = $squareHouse;

        return $this;
    }

    /**
     * Get squareHouse
     *
     * @return float 
     */
    public function getSquareHouse()
    {
        return $this->squareHouse;
    }

    /**
     * Set squareEarth
     *
     * @param float $squareEarth
     * @return ChHouse
     */
    public function setSquareEarth($squareEarth)
    {
        $this->squareEarth = $squareEarth;

        return $this;
    }

    /**
     * Get squareEarth
     *
     * @return float 
     */
    public function getSquareEarth()
    {
        return $this->squareEarth;
    }

    /**
     * Set distanceToCity
     *
     * @param float $distanceToCity
     * @return ChHouse
     */
    public function setDistanceToCity($distanceToCity)
    {
        $this->distanceToCity = $distanceToCity;

        return $this;
    }

    /**
     * Get distanceToCity
     *
     * @return float 
     */
    public function getDistanceToCity()
    {
        return $this->distanceToCity;
    }

    /**
     * Set convinient
     *
     * @param integer $convinient
     * @return ChHouse
     */
    public function setConvinient($convinient)
    {
        $this->convinient = $convinient;

        return $this;
    }

    /**
     * Get convinient
     *
     * @return integer 
     */
    public function getConvinient()
    {
        return $this->convinient;
    }

    /**
     * Set canalization
     *
     * @param integer $canalization
     * @return ChHouse
     */
    public function setCanalization($canalization)
    {
        $this->canalization = $canalization;

        return $this;
    }

    /**
     * Get canalization
     *
     * @return integer 
     */
    public function getCanalization()
    {
        return $this->canalization;
    }

    /**
     * Set water
     *
     * @param integer $water
     * @return ChHouse
     */
    public function setWater($water)
    {
        $this->water = $water;

        return $this;
    }

    /**
     * Get water
     *
     * @return integer 
     */
    public function getWater()
    {
        return $this->water;
    }

    /**
     * Set electricity
     *
     * @param integer $electricity
     * @return ChHouse
     */
    public function setElectricity($electricity)
    {
        $this->electricity = $electricity;

        return $this;
    }

    /**
     * Get electricity
     *
     * @return integer 
     */
    public function getElectricity()
