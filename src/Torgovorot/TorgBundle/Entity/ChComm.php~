<?php

namespace Torgovorot\TorgBundle\Entity;

use Doctrine\ORM\Mapping as ORM;

/**
 * ChComm
 */
class ChComm
{
    /**
     * @var integer
     */
    private $commType;

    /**
     * @var float
     */
    private $squareEarth;

    /**
     * @var float
     */
    private $squarePlace;

    /**
     * @var integer
     */
    private $videoId;

    /**
     * @var integer
     */
    private $photoId;

    /**
     * @var integer
     */
    private $id;


    /**
     * Set commType
     *
     * @param integer $commType
     * @return ChComm
     */
    public function setCommType($commType)
    {
        $this->commType = $commType;

        return $this;
    }

    /**
     * Get commType
     *
     * @return integer 
     */
    public function getCommType()
    {
        return $this->commType;
    }

    /**
     * Set squareEarth
     *
     * @param float $squareEarth
     * @return ChComm
     */
    public function setSquareEarth($squareEarth)
    {
        $this->squareEarth = $squareEarth;

        return $this;
    }

    /**
     * Get squareEarth
     *
     * @return float 
     */
    public function getSquareEarth()
    {
        return $this->squareEarth;
    }

    /**
     * Set squarePlace
     *
     * @param float $squarePlace
     * @return ChComm
     */
    public function setSquarePlace($squarePlace)
    {
        $this->squarePlace = $squarePlace;

        return $this;
    }

    /**
     * Get squarePlace
     *
     * @return float 
     */
    public function getSquarePlace()
    {
        return $this->squarePlace;
    }

    /**
     * Set videoId
     *
     * @param integer $videoId
     * @return ChComm
     */
    public function setVideoId($videoId)
    {
        $this->videoId = $videoId;

        return $this;
    }

    /**
     * Get videoId
     *
     * @return integer 
     */
    public function getVideoId()
    {
        return $this->videoId;
    }

    /**
     * Set photoId
     *
     * @param integer $photoId
     * @return ChComm
     */
    public function setPhotoId($photoId)
    {
        $this->photoId = $photoId;

        return $this;
    }

    /**
     * Get photoId
     *
     * @return integer 
     */
    public function getPhotoId()
    {
        return $this->photoId;
    }

    /**
     * Get id
     *
     * @return integer 
     */
    public function getId()
    {
        return $this->id;
    }
}
