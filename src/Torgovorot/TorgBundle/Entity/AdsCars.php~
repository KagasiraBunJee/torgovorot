     * @var \DateTime
     */
    private $updated;

    /**
     * @var integer
     */
    private $vip;

    /**
     * @var integer
     */
    private $premium;

    /**
     * @var integer
     */
    private $emergency;

    /**
     * @var integer
     */
    private $views;

    /**
     * @var integer
     */
    private $special;


    /**
     * Set time
     *
     * @param \DateTime $time
     * @return AdsCars
     */
    public function setTime($time)
    {
        $this->time = $time != "" ? $time : \DateTime("now");

        return $this;
    }

    /**
     * Get time
     *
     * @return \DateTime 
     */
    public function getTime()
    {
        return $this->time;
    }

    /**
     * Set updated
     *
     * @param \DateTime $updated
     * @return AdsCars
     */
    public function setUpdated($updated)
    {
        $this->updated = $updated != "" ? $updated : new \DateTime("now");

        return $this;
    }

    /**
     * Get updated
     *
     * @return \DateTime 
     */
    public function getUpdated()
    {
        return $this->updated;
    }

    /**
     * Set vip
     *
     * @param integer $vip
     * @return AdsCars
     */
    public function setVip($vip)
    {
        $this->vip = $vip != "" ? $vip : 0;

        return $this;
    }

    /**
     * Get vip
     *
     * @return integer 
     */
    public function getVip()
    {
        return $this->vip;
    }

    /**
     * Set premium
     *
     * @param integer $premium
     * @return AdsCars
     */
    public function setPremium($premium)
    {
        $this->premium = $premium != "" ? $premium : 0;

        return $this;
    }

    /**
     * Get premium
     *
     * @return integer 
     */
    public function getPremium()
    {
        return $this->premium;
    }

    /**
     * Set emergency
     *
     * @param integer $emergency
     * @return AdsCars
     */
    public function setEmergency($emergency)
    {
        $this->emergency = 0;

        return $this;
    }

    /**
     * Get emergency
     *
     * @return integer 
     */
    public function getEmergency()
    {
        return $this->emergency;
    }

    /**
     * Set views
     *
     * @param integer $views
     * @return AdsCars
     */
    public function setViews($views)
