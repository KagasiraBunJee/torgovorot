<?php

namespace Torgovorot\TorgBundle\Entity;

use Doctrine\ORM\Mapping as ORM;

/**
 * Pages
 */
class Pages
{
    /**
     * @var string
     */
    private $title;

    /**
     * @var string
     */
    private $chpu;

    /**
     * @var string
     */
    private $url;

    /**
     * @var string
     */
    private $content;

    /**
     * @var integer
     */
    private $id;


    /**
     * Set title
     *
     * @param string $title
     * @return Pages
     */
    public function setTitle($title)
    {
        $this->title = $title;

        return $this;
    }

    /**
     * Get title
     *
     * @return string 
     */
    public function getTitle()
    {
        return $this->title;
    }

    /**
     * Set chpu
     *
     * @param string $chpu
     * @return Pages
     */
    public function setChpu($chpu)
    {
        $this->chpu = $chpu;

        return $this;
    }

    /**
     * Get chpu
     *
     * @return string 
     */
    public function getChpu()
    {
        return $this->chpu;
    }

    /**
     * Set url
     *
     * @param string $url
     * @return Pages
     */
    public function setUrl($url)
    {
        $this->url = $url;

        return $this;
    }

    /**
     * Get url
     *
     * @return string 
     */
    public function getUrl()
    {
        return $this->url;
    }

    /**
     * Set content
     *
     * @param string $content
     * @return Pages
     */
    public function setContent($content)
    {
        $this->content = $content;

        return $this;
    }

    /**
     * Get content
     *
     * @return string 
     */
    public function getContent()
    {
        return $this->content;
    }

    /**
     * Get id
     *
     * @return integer 
     */
    public function getId()
    {
        return $this->id;
    }
    /**
     * @var integer
     */
    private $topMenu;

    /**
     * @var integer
     */
    private $bottomMenu;


    /**
     * Set topMenu
     *
     * @param integer $topMenu
     * @return Pages
     */
    public function setTopMenu($topMenu)
    {
        $this->topMenu = $topMenu;

        return $this;
    }

    /**
     * Get topMenu
     *
     * @return integer 
     */
    public function getTopMenu()
    {
        return $this->topMenu;
    }

    /**
     * Set bottomMenu
     *
     * @param integer $bottomMenu
     * @return Pages
     */
    public function setBottomMenu($bottomMenu)
    {
        $this->bottomMenu = $bottomMenu;

        return $this;
    }

    /**
     * Get bottomMenu
     *
     * @return integer 
     */
    public function getBottomMenu()
    {
        return $this->bottomMenu;
    }
    /**
     * @var integer
     */
    private $mainPage;

    /**
     * @var integer
     */
    private $undeleted;


    /**
     * Set mainPage
     *
     * @param integer $mainPage
     * @return Pages
     */
    public function setMainPage($mainPage)
    {
        $this->mainPage = $mainPage;

        return $this;
    }

    /**
     * Get mainPage
     *
     * @return integer 
     */
    public function getMainPage()
    {
        return $this->mainPage;
    }

    /**
     * Set undeleted
     *
     * @param integer $undeleted
     * @return Pages
     */
    public function setUndeleted($undeleted)
    {
        $this->undeleted = $undeleted;

        return $this;
    }

    /**
     * Get undeleted
     *
     * @return integer 
     */
    public function getUndeleted()
    {
        return $this->undeleted;
    }
}
