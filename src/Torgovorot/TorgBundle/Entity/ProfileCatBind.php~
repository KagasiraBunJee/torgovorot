<?php

namespace Torgovorot\TorgBundle\Entity;

use Doctrine\ORM\Mapping as ORM;

/**
 * ProfileCatBind
 */
class ProfileCatBind
{
    /**
     * @var integer
     */
    private $mainId;

    /**
     * @var integer
     */
    private $podId;

    /**
     * @var integer
     */
    private $id;


    /**
     * Set mainId
     *
     * @param integer $mainId
     * @return ProfileCatBind
     */
    public function setMainId($mainId)
    {
        $this->mainId = $mainId;

        return $this;
    }

    /**
     * Get mainId
     *
     * @return integer 
     */
    public function getMainId()
    {
        return $this->mainId;
    }

    /**
     * Set podId
     *
     * @param integer $podId
     * @return ProfileCatBind
     */
    public function setPodId($podId)
    {
        $this->podId = $podId;

        return $this;
    }

    /**
     * Get podId
     *
     * @return integer 
     */
    public function getPodId()
    {
        return $this->podId;
    }

    /**
     * Get id
     *
     * @return integer 
     */
    public function getId()
    {
        return $this->id;
    }
}
