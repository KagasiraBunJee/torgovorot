<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Torgovorot\TorgBundle\Entity\AdsEvents" table="ads_events">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="title" type="string" column="title" length="255" nullable="true"/>
    <field name="shortDescr" type="text" column="short_descr" nullable="true"/>
    <field name="description" type="text" column="description" nullable="true"/>
    <field name="ageRestrict" type="string" column="age_restrict" length="255" nullable="true"/>
    <field name="categoryIds" type="string" column="category_ids" length="255" nullable="true"/>
    <field name="photoIds" type="string" column="photo_ids" length="255" nullable="true"/>
    <field name="eventTime" type="datetime" column="event_time" nullable="true"/>
    <field name="endEvent" type="datetime" column="end_event" nullable="true"/>
    <field name="time" type="datetime" column="time" nullable="true"/>
    <field name="updateTime" type="datetime" column="update_time" nullable="true"/>
    <field name="recommend" type="integer" column="recommend" nullable="true"/>
    <field name="ownerId" type="integer" column="owner_id" nullable="true"/>
    <field name="emergency" type="integer" column="emergency" nullable="true"/>
    <field name="premium" type="integer" column="premium" nullable="true"/>
    <field name="vip" type="integer" column="vip" nullable="true"/>
    <field name="views" type="integer" column="views" nullable="true"/>
    <field name="special" type="integer" column="special" nullable="true"/>
    <field name="adsState" type="integer" column="ads_state" nullable="true"/>
    <field name="town" type="string" column="town" length="255" nullable="true"/>
    <field name="addrId" type="string" column="addr_id" length="60" nullable="true"/>
    <field name="anything" type="text" column="anything" nullable="true"/>
    <field name="length" type="integer" column="length" nullable="true"/>
    <field name="genre" type="string" column="genre" length="255" nullable="true"/>
    <field name="director" type="string" column="director" length="255" nullable="true"/>
  </entity>
</doctrine-mapping>
