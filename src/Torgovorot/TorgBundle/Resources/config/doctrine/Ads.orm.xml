<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Torgovorot\TorgBundle\Entity\Ads" table="ads">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="ownerId" type="integer" column="owner_id" nullable="true"/>
    <field name="title" type="string" column="title" length="255" nullable="true"/>
    <field name="description" type="text" column="description" nullable="true"/>
    <field name="time" type="datetime" column="time" nullable="true"/>
    <field name="categoryId" type="integer" column="category_id" nullable="true"/>
    <field name="price" type="float" column="price" precision="10" scale="0" nullable="true"/>
    <field name="updated" type="datetime" column="updated" nullable="true"/>
    <field name="recommend" type="integer" column="recommend" nullable="true"/>
    <field name="days" type="integer" column="days" nullable="true"/>
    <field name="atype" type="integer" column="atype" nullable="true"/>
    <field name="stype" type="integer" column="stype" nullable="true"/>
  </entity>
</doctrine-mapping>
