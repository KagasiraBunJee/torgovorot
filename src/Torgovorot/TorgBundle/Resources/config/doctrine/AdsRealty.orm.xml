<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Torgovorot\TorgBundle\Entity\AdsRealty" table="ads_realty">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="ownerId" type="integer" column="owner_id" nullable="true"/>
    <field name="title" type="string" column="title" length="255" nullable="true"/>
    <field name="description" type="text" column="description" nullable="true"/>
    <field name="time" type="datetime" column="time" nullable="true"/>
    <field name="categoryId" type="integer" column="category_id" nullable="true"/>
    <field name="price" type="float" column="price" precision="10" scale="0" nullable="true"/>
    <field name="updated" type="datetime" column="updated" nullable="true"/>
    <field name="recommend" type="integer" column="recommend" nullable="true"/>
    <field name="days" type="integer" column="days" nullable="true"/>
    <field name="addrId" type="integer" column="addr_id" nullable="true"/>
    <field name="adsType" type="integer" column="ads_type" nullable="true"/>
    <field name="chId" type="integer" column="ch_id" nullable="true"/>
    <field name="otherType" type="string" column="other_type" length="255" nullable="true"/>
    <field name="photoIds" type="string" column="photo_ids" length="255" nullable="true"/>
    <field name="emergency" type="integer" column="emergency" nullable="true"/>
    <field name="premium" type="integer" column="premium" nullable="true"/>
    <field name="vip" type="integer" column="vip" nullable="true"/>
    <field name="views" type="integer" column="views" nullable="true"/>
    <field name="special" type="integer" column="special" nullable="true"/>
    <field name="shortDesc" type="text" column="short_desc" nullable="true"/>
    <field name="adsState" type="integer" column="ads_state" nullable="true"/>
    <field name="town" type="string" column="town" length="255" nullable="true"/>
  </entity>
</doctrine-mapping>
